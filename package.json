{
  "name": "dldldl",
  "version": "1.0.2",
  "description": "A music syncing utility",
  "repository": "github:andrasore/dldldl",
  "type": "module",
  "scripts": {
    "start": "node --experimental-transform-types src/cli.ts",
    "build": "tsc",
    "test": "node ./test/smoketest.js",
    "prettier:check": "prettier . -c",
    "prettier:fix": "prettier . -w",
    "all": "npm run prettier:check && npm run build && npm run test"
  },
  "bin": {
    "dldldl": "src/cli.js"
  },
  "author": "Andras Sore",
  "license": "ISC",
  "dependencies": {
    "@distube/ytdl-core": "latest",
    "commander": "^12.1.0",
    "ffmpeg-static": "^5.2.0",
    "fs-extra": "^11.2.0",
    "listr2": "^8.2.3",
    "p-queue": "^8.0.1",
    "soundcloud.ts": "^0.6.1",
    "ytpl": "^2.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.5.5",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  }
}
